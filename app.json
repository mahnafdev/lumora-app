{
	"expo": {
		"name": "Lumora",
		"slug": "lumora-app",
		"version": "0.3.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "lumoraapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#03070e"
			},
			"edgeToEdgeEnabled": true
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#03070e"
				}
			],
			"expo-secure-store"
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
